generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id         Int      @unique @default(autoincrement())
  firstName  String
  secondName String
  email      String
  password   String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
